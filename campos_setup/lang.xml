<?xml version='1.0' encoding='UTF-8'?>
<openerp>
  <data noupdate="1">

    <record model="base.language.install" id="install_da">
        <field name="lang">da_DK</field>
        <field name="state">done</field>
        <field name="overwrite" eval="1"/>
    </record>

	<record id="base.user_root" model="res.users">
            <field name="tz" eval="'Europe/Copenhagen'"/>
    </record>
        
    <function
        model="base.language.install"
        name="lang_install"
         eval="[[ref('install_da')]]"/>
  </data>
</openerp>